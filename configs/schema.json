{
    "$defs": {
        "DatasetConfig": {
            "properties": {
                "name": {
                    "title": "Name",
                    "type": "string"
                },
                "root_aptos": {
                    "title": "Root Aptos",
                    "type": "string"
                },
                "root_ddr": {
                    "title": "Root Ddr",
                    "type": "string"
                },
                "split": {
                    "maxItems": 2,
                    "minItems": 2,
                    "prefixItems": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "title": "Split",
                    "type": "array"
                },
                "num_keypoints": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Num Keypoints"
                },
                "sift_sigma": {
                    "anyOf": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Sift Sigma"
                },
                "distance_sigma_px": {
                    "title": "Distance Sigma Px",
                    "type": "number"
                },
                "which_features": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Which Features"
                },
                "feature_layer": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Feature Layer"
                },
                "features_reduction": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": "mean",
                    "title": "Features Reduction"
                },
                "reinterpolation": {
                    "anyOf": [
                        {
                            "maxItems": 2,
                            "minItems": 2,
                            "prefixItems": [
                                {
                                    "type": "integer"
                                },
                                {
                                    "type": "integer"
                                }
                            ],
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Reinterpolation"
                }
            },
            "required": [
                "name",
                "root_aptos",
                "root_ddr",
                "split",
                "distance_sigma_px"
            ],
            "title": "DatasetConfig",
            "type": "object"
        },
        "ModelConfig": {
            "properties": {
                "gnn_hidden_dim": {
                    "title": "Gnn Hidden Dim",
                    "type": "integer"
                },
                "num_layers": {
                    "title": "Num Layers",
                    "type": "integer"
                },
                "sortpool_k": {
                    "title": "Sortpool K",
                    "type": "integer"
                },
                "conv_hidden_dims": {
                    "maxItems": 2,
                    "minItems": 2,
                    "prefixItems": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Conv Hidden Dims",
                    "type": "array"
                },
                "compile": {
                    "title": "Compile",
                    "type": "boolean"
                },
                "lr": {
                    "anyOf": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": 0.001,
                    "title": "Lr"
                },
                "weight_decay": {
                    "anyOf": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": 0.01,
                    "title": "Weight Decay"
                },
                "optimizer_algo": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": "adamw",
                    "title": "Optimizer Algo"
                },
                "loss_type": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": "CE",
                    "title": "Loss Type"
                }
            },
            "required": [
                "gnn_hidden_dim",
                "num_layers",
                "sortpool_k",
                "conv_hidden_dims",
                "compile"
            ],
            "title": "ModelConfig",
            "type": "object"
        }
    },
    "properties": {
        "dataset": {
            "$ref": "#/$defs/DatasetConfig"
        },
        "model": {
            "$ref": "#/$defs/ModelConfig"
        },
        "batch_size": {
            "title": "Batch Size",
            "type": "integer"
        },
        "max_epochs": {
            "title": "Max Epochs",
            "type": "integer"
        },
        "seed": {
            "title": "Seed",
            "type": "integer"
        },
        "project_name": {
            "title": "Project Name",
            "type": "string"
        },
        "tag": {
            "title": "Tag",
            "type": "string"
        }
    },
    "required": [
        "dataset",
        "model",
        "batch_size",
        "max_epochs",
        "seed",
        "project_name",
        "tag"
    ],
    "title": "Config",
    "type": "object"
}